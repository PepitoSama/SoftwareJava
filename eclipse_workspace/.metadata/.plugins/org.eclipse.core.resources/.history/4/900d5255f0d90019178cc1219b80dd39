import java.util.ArrayList;

public class Main {
	public static void main(String[] args) {
		ArrayList<Compteur> compteurs = new ArrayList<Compteur>();
		for(int i = 0; i<50; i++) {
			compteurs.add(new Compteur("Toto " + i+1, 10));
		}
		
		for(int i = 0; i<50; i++) {
			compteurs.get(i).run();
		}
		
	}
}
