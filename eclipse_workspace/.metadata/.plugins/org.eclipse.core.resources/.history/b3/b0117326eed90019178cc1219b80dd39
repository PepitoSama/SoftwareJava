
public class Compteur {
	
	private String name;
	private int n;
	
	// CONSTRUCTOR
	public Compteur(String name, int n) {
		this.name = name;
		this.n = n;
	}
	
	public String compter() {
		for(int i = 0; i < this.n; i++) {
			sleep((int)(Math.random() * 100)); 
		}
		return this.name + " a fini de compter jusqu'Ã  " + this.n
	}
}
